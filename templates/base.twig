{% block html_head_container %}
  {% include 'html-header.twig' %}
	{% block head %}{% endblock %}
</head>
{% endblock %}

<body class="{{ body_class }} {% block body_extra_class %}{% endblock %}">

  {{ function('mill3wp_body_start') }}

  {# {% include 'power-mode.twig' %} #}
  {% include 'site-loader.twig' %}
  {% include 'site-transition.twig' %}
  {# {% include 'site-webgl.twig' %} #}

  <div data-windmill="wrapper">
    <div
      data-windmill="container"
      data-ui="gform{#,mouse-wheel-vimeo,mouse-wheel-youtube,mouse-wheel-facebook#}"
    >

      {% block header %}
        {% include 'site-header.twig' with {'menu': header_navigation.items, 'site': site, 'options': options} only %}
        {% include 'site-nav.twig' with {'menu': site_navigation.items, 'site': site, 'options': options} only %}
      {% endblock %}

      <main class="flex-grow-1" role="main">
        {% block content %}
          Sorry, no content :(
        {% endblock %}
      </main>

      {% block footer %}
        {# {% include 'partial/newsletter.twig' %} #}
        {% include 'site-footer.twig' %}
      {% endblock %}

      {# {% include 'site-search.twig' %} #}

    </div>
  </div>

  {# {% include 'site-video.twig' %} #}
  {% include 'gdpr.twig' with {'site': site, 'options': options} only %}

  {{ function('wp_footer') }}
</body>
</html>
